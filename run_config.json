{
    "run_type": "staged_learning",
    "device": "cpu",
    "use_cuda": true,
    "tb_logdir": "tb_logs",
    "json_out_config": "run_config.json",
    "seed": null,
    "parameters": {
        "prior_file": "reinvent.prior",
        "agent_file": "reinvent.prior",
        "summary_csv_prefix": "results/ppar_rl",
        "use_checkpoint": false,
        "purge_memories": false,
        "smiles_file": null,
        "sample_strategy": "multinomial",
        "distance_threshold": 99999,
        "batch_size": 32,
        "randomize_smiles": true,
        "unique_sequences": false,
        "temperature": 1.0,
        "tb_isim": false
    },
    "scoring": null,
    "scheduler": null,
    "responder": null,
    "filter": null,
    "stage": [
        {
            "max_steps": 100,
            "max_score": 1.0,
            "chkpt_file": "results/stage1.chkpt",
            "termination": "simple",
            "min_steps": 50,
            "scoring": {
                "type": "geometric_mean",
                "component": [
                    {
                        "DockStream": {
                            "endpoint": [
                                {
                                    "name": "PPARa Vina Score",
                                    "weight": 0.8,
                                    "params": {
                                        "configuration_path": "dockstream_config.json",
                                        "docker_script_path": "/home/michael-kaiser/DockStream/docker.py",
                                        "docker_python_path": "/home/michael-kaiser/anaconda3/envs/DockStream/bin/python"
                                    },
                                    "transform": {
                                        "type": "reverse_sigmoid",
                                        "high": -6.0,
                                        "low": -12.0,
                                        "k": 0.25
                                    }
                                }
                            ]
                        }
                    },
                    {
                        "QED": {
                            "endpoint": [
                                {
                                    "name": "Drug-likeness",
                                    "weight": 0.1
                                }
                            ]
                        }
                    },
                    {
                        "custom_alerts": {
                            "endpoint": [
                                {
                                    "name": "Structural Alerts",
                                    "weight": 1.0,
                                    "params": {
                                        "smarts": [
                                            "[*;r8]",
                                            "[*;r9]",
                                            "[*;r10]",
                                            "[#8][#8]",
                                            "[#6;+]",
                                            "[#16][#16]",
                                            "C#C",
                                            "C(=[O,S])[O,S]"
                                        ]
                                    }
                                }
                            ]
                        }
                    },
                    {
                        "MolecularWeight": {
                            "endpoint": [
                                {
                                    "name": "MW",
                                    "weight": 0.1,
                                    "transform": {
                                        "type": "double_sigmoid",
                                        "high": 500.0,
                                        "low": 200.0,
                                        "coef_div": 500.0,
                                        "coef_si": 20.0,
                                        "coef_se": 20.0
                                    }
                                }
                            ]
                        }
                    }
                ],
                "filename": null
            },
            "diversity_filter": null
        }
    ],
    "learning_strategy": {
        "type": "dap",
        "sigma": 128,
        "rate": 0.0001
    },
    "diversity_filter": {
        "type": "IdenticalMurckoScaffold",
        "bucket_size": 25,
        "minscore": 0.4,
        "minsimilarity": 0.4,
        "penalty_multiplier": 0.5
    },
    "intrinsic_penalty": null,
    "inception": null
}